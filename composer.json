{
    "name": "gustavobgama/metatrader5report-transformer",
    "description": "This project does the transformation and conversion of the HTML Metatrader 5 report. It imports the report, transform data and expose the results as an HTTP call.",
    "type": "project",
    "require": {
        "league/route": "^5.1",
        "laminas/laminas-diactoros": "^2.7",
        "symfony/console": "^5.3",
        "symfony/dom-crawler": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/css-selector": "^5.3",
        "illuminate/database": "^8.61",
        "lulco/phoenix": "^1.10",
        "laminas/laminas-httphandlerrunner": "^2.0",
        "php-di/php-di": "^6.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "GustavoGama\\MetatraderReportTransformer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gustavo Gama",
            "email": "gustavobgama@gmail.com"
        }
    ],
    "require-dev": {
        "symfony/var-dumper": "^5.3"
    }
}
